],
  "Layers":[
    {
      "id":1,
      "title": "Sequence",
      "info": [
        {
          "name": "mainParams",
          "params": [
            {
              "kind":"input",
              "title":"units",
              "type": "number",
              "min":1,
              "default":1,
              "step":1
            }, 
            {
              "kind":"array",
              "title":"inputShape",
              "type":"text",
              "placeholder":"[ , ...]",
              "default":[1]
            }, 
            {
              "kind":"select",
              "title":"activation",
              "list": [
                "elu","hardSigmoid","linear","relu","relu6", "selu",
                "sigmoid","softmax","softplus","softsign","tanh",
                "swish","mish"
              ],
              "default":"linear"
            },
            {
              "kind":"select",
              "title":"useBias",
              "list": ["true", "false"],
              "default":true
            }

          ]
        },
        {
          "name": "subParams",
          "params": [
            {
              "kind":"array",
              "title":"batchInputShape",
              "type":"text",
              "placeholder":"[null, 1]"
            },
            { "kind":"select",
              "title":"kerneldefaultializer",
              "list": [
                "constant","glorotNormal","glorotUniform","heNormal",
                "heUniform","identity", "leCunNormal","leCunUniform",
                "ones","orthogonal","randomNormal", "randomUniform",
                "truncatedNormal","varianceScaling","zeros"
              ]
            },
            {
              "kind":"select",
              "title":"biasdefaultializer",
              "list": [
                "constant","glorotNormal","glorotUniform","heNormal",
                "heUniform","identity", "leCunNormal","leCunUniform",
                "ones","orthogonal","randomNormal", "randomUniform",
                "truncatedNormal","varianceScaling","zeros"
              ]
            },
            {
              "kind":"input",
              "title":"inputDim",
              "min":1
            }
          ]}
      ]
    },
    {
      "id":2,
      "title": "Model",
      "info": ["units", "shape", "activation"]
    }  
  ],
  "Parameters":[
    {
      "kind":"input",
      "title":"batchSize",
      "type":"number",
      "placeholder":"number",
      "min":1,
      "default":32
    },
    {
      "kind":"input",
      "title":"epochs",
      "type":"number",
      "placeholder":"number",
      "min":1,
      "default":50
    }
  ]
